<turbo-frame id="aiContent">
  <div id="app" class="card">
    <p>{{ message }}</p>
  </div>
  <script type="module" defer>
    import { createApp, ref } from 'https://unpkg.com/vue@3/dist/vue.esm-browser.js'

    createApp({
      setup() {
        const message = ref('Hello vue!')
        return {
          message
        }
      }
    }).mount('#app')
  </script>
</turbo-frame>

